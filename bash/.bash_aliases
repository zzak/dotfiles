alias dbundle='ruby -I ~/projects/bundler/lib ~/projects/bundler/bin/bundle'
alias be='bundle exec'
alias beer='bundle exec rake'
alias gc='git commit -e --verbose'
alias l='ls -lX --group-directories-first'
alias m='myougiden'
alias yube="youtube-dl -f 'bestvideo+bestaudio' --merge-output-format mp4 --sleep-interval 5 --socket-timeout 60 --output '%(channel).5s-%(title).20s-%(id).8s.%(ext)s'"
alias yt-song='youtube-dl -f "bestaudio" -x --sleep-interval 5 --socket-timeout 60'
alias yt='yube'
#alias rm='trash'
alias ed='emacs --daemon'
alias em='emacsclient -nw'
alias ec='emacsclient -cn'
#alias vi='em'
#alias vim='em'
alias cdtmp='cd `mktemp -d`'
alias vitmp='vi `mktemp -d`'
alias emtmp='em `mktemp -d`'
alias x='xclip -selection clipboard'
alias repl='lein trampoline repl :headless &'
alias emupt='ps -p `pidof emacs` -o etime'
